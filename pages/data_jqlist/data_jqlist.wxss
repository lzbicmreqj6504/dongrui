    width: 95%;
    margin: 0 auto;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
.yx .wx-radio-input.wx-radio-input-checked::before{
    border-radius: 50%;
   width: 25rpx; 
   height: 25rpx; 
   text-align: center;
   font-size:0rpx; /* 对勾大小 去掉 */
   background-color: #18d77f; 
