 .radio-group{
     margin-top: 30rpx;
 }
 .data_serch{
     width: 85%;
     margin: 30rpx auto;
     display: flex;
     flex-direction: column;

 }
 .serch_list{
     width: 100%;
     box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
     height: 50vh;
     border-radius: 15rpx;
     padding: 30rpx;
 }
 .title_wo{
     width: 60%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
 }
 .list_title{
     width: 100%;
     display: flex;
     justify-content: space-between;
     color:#666666;
 }
 .title_card{
     width: 40%;
     height: 50rpx;
     text-align: center;
     background-color: #999999;
     border-radius: 25rpx;
 }
 .mid{
     height:170rpx;
     border-bottom: 1px solid #999999;
     display: flex;
     flex-direction: column;
     justify-content: space-around;
     color:#676869;
 }
 .bot{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    color:#676869;
 }
 .bot .bot_view{
     margin-top: 20rpx;
     display: flex;
     justify-content: space-between;
 }
 .w3-border-0{border:0!important}.w3-border{border:1px solid #ccc!important;margin-top: 20rpx;}
.w3-border-top{border-top:1px solid #ccc!important}.w3-border-bottom{border-bottom:1px solid #ccc!important}
.w3-border-left{border-left:1px solid #ccc!important}.w3-border-right{border-right:1px solid #ccc!important}
.w3-grey,.w3-hover-grey:hover,.w3-gray,.w3-hover-gray:hover{color:#f7bd3d!important;background-color:#f7bd3d!important}

